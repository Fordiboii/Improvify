                <TableColumn fx:id="dateCol" prefWidth="164.0" text="Date" />

                <TableColumn fx:id="intensityCol" minWidth="0.0" prefWidth="88.0" text="Intensity" />

                <TableColumn fx:id="kCalCol" prefWidth="71.0" text="KCal" />

                <TableColumn fx:id="heartrateCol" prefWidth="85.0" text="HeartRate" />

                <TableColumn fx:id="maxHeartRateCol" prefWidth="105.0" text="MaxHR" />
