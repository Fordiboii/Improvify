<Pane prefHeight="750.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tdt4140.gr1817.app.ui.feature.workoutsession.WorkoutsessionController">
    <children>
        <Label layoutX="230.0" layoutY="31.0" text="Workout Session">
            <font>
                <Font size="42.0" />
            </font>
        </Label>
        <TableView fx:id="workoutTable" layoutX="75.0" layoutY="145.0" prefHeight="394.0" prefWidth="693.0">
            <columns>
                <TableColumn fx:id="dateCol" prefWidth="115.0" sortable="false" text="Date" />

                <TableColumn fx:id="intensityCol" minWidth="0.0" prefWidth="94.0" sortable="false" text="Intensity" />

                <TableColumn fx:id="kCalCol" prefWidth="97.0" sortable="false" text="KCal" />

                <TableColumn fx:id="heartrateCol" prefWidth="102.0" sortable="false" text="HeartRate" />

                <TableColumn fx:id="maxHeartRateCol" prefWidth="105.0" sortable="false" text="MaxHR" />
                <TableColumn fx:id="distanceCol" prefWidth="89.0" sortable="false" text="Distance" />
                <TableColumn fx:id="durationCol" prefWidth="92.0" sortable="false" text="Duration" />
            </columns>
        </TableView>

        <Button mnemonicParsing="false" onAction="#goToViewUser" text="Back" />
      <Button layoutX="672.0" layoutY="604.0" mnemonicParsing="false" text="Select" />
      <TextField layoutX="86.0" layoutY="604.0" prefHeight="31.0" prefWidth="253.0" promptText="From (Date format: yyyy-MM-dd)" />
      <TextField layoutX="386.0" layoutY="604.0" prefHeight="31.0" prefWidth="229.0" promptText="To (Date format: yyyy-MM-dd)" />

    </children>

</Pane>
